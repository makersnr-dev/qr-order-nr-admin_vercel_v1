{
  "version": 2,
  "routes": [
    {
      "src": "/api/download-proxy",
      "dest": "/api/download-proxy.js"
    },
    {
      "src": "^/login$",
      "dest": "/login.html"
    },
    {
      "src": "^/admin-config$",
      "dest": "/api/admin-config.js"
    }
  ],
  "rewrites": [
    {
      "source": "/admin-config",
      "destination": "/api/admin-config"
    },
    {
      "source": "/events/orders",
      "destination": "/api/events-orders"
    },
    {
      "source": "/daily-code",
      "destination": "/api/daily-code"
    }
  ]
}